{
    "name": "dljubas20/organizational-rbac",
    "description": "Laravel package for managing organizations, users roles and permissions.",
    "keywords": [
        "Dario Ljubas",
        "laravel",
        "organizational-rbac"
    ],
    "homepage": "https://github.com/dljubas20/organizational-rbac",
    "license": "MIT",
    "authors": [
        {
            "name": "Dario Ljubas",
            "email": "darioljubas33@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0"
    },
    "autoload": {
        "psr-4": {
            "DarioLjubas\\OrganizationalRBAC\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DarioLjubas\\OrganizationalRBAC\\ServiceProvider"
            ],
            "aliases": {
                "OrganizationalRBAC": "DarioLjubas\\OrganizationalRBAC\\Facades\\OrganizationalRBAC"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}